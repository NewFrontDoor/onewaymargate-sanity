{
	"name": "onewaymargateorg",
	"private": true,
	"version": "1.0.0",
	"description": "",
	"main": "package.json",
	"author": "New Front Door <info@newfrontdoor.org>",
	"license": "UNLICENSED",
	"scripts": {
		"start": "sanity start",
		"test": "sanity check",
		"postinstall": "patch-package"
	},
	"keywords": [
		"sanity"
	],
	"dependencies": {
		"@lifeomic/attempt": "^3.0.0",
		"@newfrontdoor/audio-player": "^0.3.2",
		"@newfrontdoor/bible": "^1.0.0",
		"@newfrontdoor/s3-file-upload": "^0.4.3",
		"@sanity/base": "^2.30.5",
		"@sanity/color-input": "^2.30.5",
		"@sanity/components": "^2.14.0",
		"@sanity/core": "^2.30.5",
		"@sanity/dashboard": "^2.30.5",
		"@sanity/default-layout": "^2.30.5",
		"@sanity/default-login": "^2.30.5",
		"@sanity/desk-tool": "^2.30.5",
		"@sanity/google-maps-input": "^2.30.5",
		"@sanity/vision": "^2.30.5",
		"aws-sdk": "^2.550.0",
		"prop-types": "^15.7.2",
		"react": "^16.2",
		"react-dom": "^16.2",
		"react-dropzone": "^10.1.8",
		"react-icons": "^3.7.0",
		"react-spinners": "^0.11.0",
		"sanity-plugin-dashboard-widget-feed": "0.1.3",
		"speakingurl": "^14.0.1",
		"styled-components": "^5.2.0",
		"theme-ui": "^0.3.5"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.3",
		"eslint": "^6.3.0",
		"eslint-config-xo-react": "^0.20.0",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^2.0.1",
		"patch-package": "^6.2.2",
		"prettier": "^1.18.2",
		"xo": "^0.25.3"
	},
	"xo": {
		"extends": [
			"xo-react"
		],
		"parser": "babel-eslint",
		"prettier": true,
		"space": true,
		"plugins": [
			"unicorn"
		],
		"rules": {
			"unicorn/filename-case": "off"
		},
		"overrides": [
			{
				"files": "**/__tests__/**/*.js",
				"rules": {
					"import/no-unassigned-import": [
						"error",
						{
							"allow": [
								"jest-dom/extend-expect"
							]
						}
					]
				}
			}
		]
	},
	"eslint": {
		"settings": {
			"react": {
				"version": "detect"
			}
		}
	}
}
